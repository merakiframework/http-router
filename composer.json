{
    "name": "meraki/http-router",
    "description": "Maps HTTP requests to HTTP responses in PHP 8+.",
    "type": "library",
    "require": {
        "php": "^8.1",
        "doctrine/inflector": "^2.0",
        "willdurand/negotiation": "^3.1",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^2.22",
        "marcocesarato/php-conventional-changelog": "^1.16",
        "vimeo/psalm": "^5.1",
        "psalm/plugin-phpunit": "^0.18.4",
        "phpunit/phpunit": "^9.5",
        "friendsofphp/php-cs-fixer": "^3.13",
        "spatie/phpunit-watcher": "^1.23",
        "captainhook/captainhook": "^5.11",
        "ramsey/conventional-commits": "^1.3",
        "narrowspark/http-emitter": "^2.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Bishop",
            "email": "nbish11@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Meraki\\Http\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meraki\\Http\\": "tests",
            "Project\\": "examples/"
        }
    },
    "scripts": {
        "lint": "psalm",
        "test": "phpunit",
        "format": "php-cs-fixer fix -vvv",
        "develop": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch"
        ],
        "changelog": "conventional-changelog",
        "release": "conventional-changelog --commit",
        "release:patch": "conventional-changelog --patch --commit",
        "release:minor": "conventional-changelog --minor --commit",
        "release:major": "conventional-changelog --major --commit"
    },
    "config": {
        "allow-plugins": {
            "captainhook/plugin-composer": true
        }
    },
    "version": "0.8.0"
}
